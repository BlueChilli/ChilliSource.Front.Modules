{
	"name": "chillifront-modules",
	"version": "0.0.1",
	"description": "ChilliFront Library of things",
	"main": "index.js",
	"license": "MIT",
	"prettier": {
		"printWidth": 100,
		"noSemi": true,
		"trailingComma": "es5",
		"jsxBracketSameLine": true,
		"rcVerbose": true,
		"useTabs": true,
		"tabWidth": 2,
		"singleQuote": true
	},
	"devDependencies": {
		"@storybook/react": "^3.4.1",
		"@types/react": "^16.4.6",
		"@types/react-dom": "^16.0.6",
		"@types/react-widgets": "^4.1.3",
		"@types/storybook__react": "^3.0.8",
		"babel-core": "^6.26.0",
		"css-loader": "^0.28.11",
		"node-sass": "^4.8.3",
		"node-sass-chokidar": "^1.2.2",
		"npm-run-all": "^4.1.2",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0"
	},
	"dependencies": {
		"@fortawesome/fontawesome": "^1.1.8",
		"@fortawesome/fontawesome-free-solid": "^5.0.13",
		"@fortawesome/fontawesome-svg-core": "^1.2.0-14",
		"@fortawesome/free-solid-svg-icons": "^5.1.0-11",
		"@fortawesome/react-fontawesome": "^0.1.0-11",
		"axios": "^0.18.0",
		"chillifront": "git+ssh://git@github.com:BlueChilli/ChilliSource.Front.git",
		"classnames": "^2.2.6",
		"local-storage": "^1.4.2",
		"lodash": "^4.17.10",
		"mixpanel-browser": "^2.22.4",
		"moment": "^2.22.2",
		"prop-types": "^15.6.2",
		"react": "^16.3.2",
		"react-async-load-script": "^1.0.7",
		"react-burger-menu": "^2.5.2",
		"react-dom": "^16.3.2",
		"react-onclickoutside": "^6.7.1",
		"react-redux": "^5.0.7",
		"react-responsive": "^4.1.0",
		"react-router-dom": "^4.3.1",
		"react-show": "2.0.4",
		"react-widgets": "^4.4.3",
		"react-widgets-moment": "^4.0.19",
		"redux": "^3.7.2",
		"redux-create-reducer": "^1.1.2",
		"redux-form": "^7.4.2",
		"redux-promise-middleware": "^5.1.1",
		"redux-thunk": "^2.3.0",
		"reselect": "^3.0.1",
		"swagger-parser": "^4.1.0",
		"valid-url": "^1.0.9"
	},
	"scripts": {
		"storybook": "start-storybook -p 9101 -c .storybook",
		"start": "npm-run-all --parallel storybook watch-css watch-css-sb watch-css-components watch-sass-helpers",
		"build": "build-storybook -c .storybook -o build",
		"build-css": "node-sass-chokidar ./modules/  -o ./modules/",
		"watch-css": "node-sass-chokidar ./modules/ -o ./modules/ --watch",
		"build-css-sb": "node-sass-chokidar ./stories/  -o ./stories/",
		"watch-css-sb": "node-sass-chokidar ./stories/ -o ./stories/ --watch",
		"build-css-components": "node-sass-chokidar ./components/  -o ./components/",
		"watch-css-components": "node-sass-chokidar ./components/ -o ./components/ --watch",
		"build-sass-helpers": "node-sass-chokidar ./sass-helpers/  -o ./sass-helpers/",
		"watch-sass-helpers": "node-sass-chokidar ./sass-helpers/ -o ./sass-helpers/ --watch"
	}
}
